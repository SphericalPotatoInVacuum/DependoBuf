message FieldsPatternMatching (n Nat) (m Nat) {
    Succ {prev: numberm}, Zero{} => enum {
        Constructor1
        Constructor2 {
            numbern Type
            field   Type
        }
    }
    Succ {prev : numbern}, Succ {prev : numberm} => enum {
        Constructor3 {
            numbern Type
            numberm Type
        }
    }
    Zero{}, Succ {prev : numberm} => enum {
        Constructor4 {
            numberm Type
        }
    }
    *, * => enum {
        Constructor5
        Constructor6
    }
}
